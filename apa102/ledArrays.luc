module ledArrays (
    input address[5],  // ROM address
    output ledByte[8]   // ROM output
  ) {
 
  const ARRAY_SIZE = 22;
   
  sig data2[ARRAY_SIZE][8];
  
 
  always {
      
    data2 = {8b0,8b0,8b0,8b0,8b0,8b0,8b0,8b0,8h1e,8he3, 8b0, 8b0, 8b0, 8he3, 8b0, 8b0, 8h1e, 8he3, 8b0, 8b0, 8b0, 8b0};
    
    ledByte = data2[address]; // 
        
  }
}
 